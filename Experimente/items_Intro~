{$NORMAL_PRACTICE_ARROWS, 'item':127, $STIM'< < < < < $ > >'}
{$NORMAL_PRACTICE_ARROWS, 'item':128, $STIM'> > $ > > < > >'}
{$NORMAL_PRACTICE_ARROWS, 'item':129, $STIM'> > $ > < < < <'}
